public class HelloWorld\par
\{\par
    \par
    // Reverse the integer \par
    public static int Reverse(int n)\{\par
        int num=Math.Abs(n);\par
        int rev=0;\par
        while( num > 0 )\{\par
            int Digit = num%10;\par
            rev=rev*10+Digit;\par
            num=num/10;\par
        \}\par
        if(n > 0)\{\par
        return rev;\par
        \}else\{\par
            return -rev;\par
        \}\par
    \}\par
    //Reverse the Integer by recursion\par
    \par
    public static int Rev(int num,int result=0)\{\par
        if(num  == 0)\{\par
            return result;\par
        \}\par
        return Rev(num/10,result*10+num%10);\par
    \}\par
    \par
    public static void Main(string[] args)\par
    \{\par
        Console.WriteLine (Reverse(-874));\par
        Console.WriteLine(Rev(1234));\par
    \}\par
\}\par
