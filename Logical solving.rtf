    public static int SecondLarge(int[] arr)\{\par
        // basic approch \par
        int Large=int.MinValue;\par
        int SecLarge=int.MinValue;\par
        \par
        for(int i=0;i<arr.Length;i++)\{\par
            \par
            if(arr[i] > Large)\{\par
                Large=arr[i];\par
            \}\par
        \}\par
        for(int i=0;i<arr.Length;i++)\{\par
            \par
            if(arr[i] > SecLarge && arr[i]  < Large)\{\par
                \par
                SecLarge=arr[i];\par
            \}\par
        \}\par
\par
        return SecLarge;\par
    \}\par
    \par
    // By foreachloop\par
    \par
    public static int SecondLarge1(int[] arr)\{\par
        \par
        int Large=int.MinValue;\par
        int Small=int.MinValue;\par
        foreach(int num in arr)\{\par
            \par
            if(num > Large)\{\par
                Small=Large;\par
                Large=num;\par
            \}else if(num > Small && num !=Large)\{\par
                \par
                Small=num;\par
            \}\par
            \par
        \}\par
        \par
        return Small;\par
    \}\par
